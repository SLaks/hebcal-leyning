{
  "name": "@hebcal/leyning",
  "version": "1.1.3",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "keywords": [
    "torah",
    "kriyah",
    "triennial"
  ],
  "description": "Javascript Torah Reading API for Parashat HaShavua and holidays",
  "main": "./dist/index.js",
  "browser": "./dist/bundle.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hebcal/hebcal-leyning.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-leyning/issues"
  },
  "typings": "leyning.d.ts",
  "files": [
    "dist",
    "leyning.d.ts"
  ],
  "dependencies": {
    "@hebcal/core": "^1.2.3"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublish": "npm run build",
    "readme": "npx jsdoc2md dist/index.js",
    "pretest": "npm run build",
    "test": "ava"
  },
  "license": "BSD-2-Clause",
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "presets": [
          "@babel/env"
        ]
      }
    },
    "inherit": true,
    "verbose": true
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "ava": "^3.9.0",
    "core-js": "^3.6.5",
    "eslint": "^7.3.1",
    "eslint-config-google": "^0.14.0",
    "jsdoc": "^3.6.4",
    "jsdoc-to-markdown": "^5.0.3",
    "rollup": "^1.29.0",
    "rollup-plugin-terser": "^6.1.0"
  }
}
